  <div class="container">
    <h4>{{availableItemsTotal}} de {{menuLength}} chefes dispon√≠veis</h4>
    <br>
  <table class="table">
    <thead>
      <tr>
        <th>Complete name</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |chef|}}
        <tr>
          <td>
            {{#link-to 'chef' chef.name}}
            {{chef.name}}
            {{/link-to}}
          </td>
          <td id="column-two">
            <button class="float-right"> Excluir</button>
            <button class="float-right"> Editar</button>
            {{#if chef.isHere}}
              <button class="float-right" {{action 'makeUnavaliable' chef}}> Desabilitar</button>
            {{else}}
                <button class="float-right" {{action 'makeAvaliable' chef}}> Habilitar</button>
            {{/if}}
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
